{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Mongo": {
        "ConnectionString": "mongodb://rootuser:rootpassword@mongo:27017",
        "DatabaseName": "vulture"
    },
    "Kafka": {
        "Configuration": {
            "BootstrapServers": "kafka:29092",
            "GroupId": "vulture.group",
            "AutoOffsetReset": "earliest"
        },
        "Topics": {
            "Entries": "vulture.entries",
            "Claims": "vulture.claims"
        }
    },
    "Bacen":{
        "Url": "http://bacen"
    }
}