{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Mongo": {
        "ConnectionString": "mongodb://rootuser:rootpassword@mongo:27017",
        "DatabaseName": "star"
    },
    "Kafka": {
        "Configuration": {
            "BootstrapServers": "kafka:29092",
            "GroupId": "star.group",
            "AutoOffsetReset": "earliest"
        },
        "Topics": {
            "Entries": "star.entries"
        }
    },
    "Bacen":{
        "Url": "http://bacen"
    }
}